   ],
   "source": [
    "n = int(input(\"Enter a positive integer: \"))\n",
    "\n",
    "even_sum = 0\n",
    "\n",
    "for i in range(1, n + 1):\n",
    "    if i % 2 == 0: \n",
    "        even_sum += i\n",
    "\n",
    "print(f\"The sum of all even numbers between 1 and {n} is: {even_sum}\")\n"
   ]
